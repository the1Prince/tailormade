{
  "expo": {
    "name": "TailorMade",
    "slug": "tailormade",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "tailormade",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.tailormade.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "package": "app.tailormade.mobile"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-image-picker",
        { "photosPermission": "Allow TailorMade to access photos for fabric images." }
      ],
      [
        "expo-notifications",
        { "icon": "./assets/notification-icon.png", "color": "#0a0a0a" }
      ]
    ]
  }
}
